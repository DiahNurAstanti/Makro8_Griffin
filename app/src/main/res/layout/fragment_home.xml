<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tv_welcome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homeFragment">

    <ImageView
        android:id="@+id/iv_profile"
        android:layout_width="51dp"
        android:layout_height="46dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_diah"
        android:layout_width="173dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="28dp"
        android:text="Selamat Datang, Indra"
        app:layout_constraintStart_toEndOf="@+id/iv_profile"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_pencarian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="32dp"
        android:drawableLeft="@android:drawable/ic_menu_search"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_diah"
        tools:text="Cari Pekerjaan" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="42dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/notifikasi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_pelatihan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:text="Cari Pelatihan"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_pencarian" />

    <ImageView
        android:id="@+id/iv_digital"
        android:layout_width="96dp"
        android:layout_height="91dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="36dp"
        android:src="@drawable/pelatiihandigital"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_pelatihan" />

    <ImageView
        android:id="@+id/iv_digital2"
        android:layout_width="96dp"
        android:layout_height="91dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/pelatiihandigital"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_digital" />

    <TextView
        android:id="@+id/iv_kerja"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="20dp"
        android:text="Cari Pekerjaan"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_digital" />

    <ImageView
        android:id="@+id/iv_pkerjaan"
        android:layout_width="96dp"
        android:layout_height="91dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/jasakeuangan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_kerja" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="96dp"
        android:layout_height="91dp"
        android:src="@drawable/jasakeuangan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_pkerjaan" />

    <TextView
        android:id="@+id/tv_kelas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="Cari Kelas"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_pkerjaan" />

    <ImageView
        android:id="@+id/iv_kelas"
        android:layout_width="96dp"
        android:layout_height="91dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:src="@drawable/backtoschool"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_kelas" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:src="@drawable/backtoschool"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_kelas" />

    <TextView
        android:id="@+id/tv_lain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Lainnya"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toEndOf="@+id/tv_pelatihan"
        app:layout_constraintTop_toBottomOf="@+id/et_pencarian" />

    <TextView
        android:id="@+id/tv_lain2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Lainnya"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.861"
        app:layout_constraintStart_toEndOf="@+id/iv_kerja"
        app:layout_constraintTop_toBottomOf="@+id/iv_digital2" />

    <TextView
        android:id="@+id/tv_lain3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Lainnya"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.868"
        app:layout_constraintStart_toEndOf="@+id/tv_kelas"
        app:layout_constraintTop_toBottomOf="@+id/imageView38" />

</androidx.constraintlayout.widget.ConstraintLayout>